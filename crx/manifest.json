{
  "name": "Geo-Location from current url",
  "version": "0.0.1",
  "description": "Displays the current urls geo location",
  "icons": { "128": "geolocation.png" },
  "background": {
      "scripts": [
        "jquery-2.1.0.min.js",
        "background.js"
      ]
  },
  "permissions": [
      "http://localhost:9000/",
      "tabs", "<all_urls>"
  ],
  "browser_action": {
    "default_title": "",
    "default_icon": "geolocation.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
